<h2>Edit <%= @player.name %></h2>

<form action="/players/<%=@player.id%>" method="post">
  Name: <input type="text" name="name" value="<%= @player.name %>">
  Age: <input type="text" name="age" value="<%= @player.age %>">
  Experience: <input type="text" name="experience" value="<%= @player.experience%>">
  Instrument: <input type="text" name="instrument" value="<%= @player.instrument.name %>">
  <button type="submit" value="submit">submit</button>
</form>